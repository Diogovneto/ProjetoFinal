<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textViewNome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/purple_700"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Diogo Neto" />

    <TextView
        android:id="@+id/textViewTelemovel"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/teal_200"
        app:layout_constraintEnd_toEndOf="@+id/textViewNome"
        app:layout_constraintStart_toStartOf="@+id/textViewNome"
        app:layout_constraintTop_toBottomOf="@+id/textViewNome"
        tools:text="938059434" />

    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/teal_200"
        app:layout_constraintEnd_toEndOf="@+id/textViewTelemovel"
        app:layout_constraintStart_toStartOf="@+id/textViewTelemovel"
        app:layout_constraintTop_toBottomOf="@+id/textViewTelemovel"
        tools:text="dvnetoubz@gmail.com" />

    <TextView
        android:id="@+id/textViewSexo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/teal_200"
        app:layout_constraintEnd_toEndOf="@+id/textViewEmail"
        app:layout_constraintStart_toStartOf="@+id/textViewEmail"
        app:layout_constraintTop_toBottomOf="@+id/textViewEmail"
        tools:text="Masculino" />

    <TextView
        android:id="@+id/textViewCartaoCidadao"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/teal_200"
        app:layout_constraintEnd_toEndOf="@+id/textViewSexo"
        app:layout_constraintStart_toStartOf="@+id/textViewSexo"
        app:layout_constraintTop_toBottomOf="@+id/textViewSexo"
        tools:text="14537834" />

    <TextView
        android:id="@+id/textViewEspecialidade"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/teal_200"
        app:layout_constraintEnd_toEndOf="@+id/textViewCartaoCidadao"
        app:layout_constraintStart_toStartOf="@+id/textViewCartaoCidadao"
        app:layout_constraintTop_toBottomOf="@+id/textViewCartaoCidadao"
        tools:text="14537834" />

</androidx.constraintlayout.widget.ConstraintLayout>