    <TextView
        android:id="@+id/textViewPulseira"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/teal_700"
        app:layout_constraintEnd_toEndOf="@+id/textViewContribuinte"
        app:layout_constraintStart_toStartOf="@+id/textViewContribuinte"
        app:layout_constraintTop_toBottomOf="@+id/textViewContribuinte"
        tools:text="Verde" />
</androidx.constraintlayout.widget.ConstraintLayout>